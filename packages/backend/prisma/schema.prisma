// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Reminder {
  id          BigInt    @id @default(autoincrement())
  uuid        String    @unique
  title       String
  description String
  datetime    DateTime
  createdAt   DateTime
  completedAt DateTime?
}
